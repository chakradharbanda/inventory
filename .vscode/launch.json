{
  "configurations": [
    {
      "type": "java",
      "name": "JwtAuthApplication",
      "request": "launch",
      "mainClass": "com.inventory.inventory.JwtAuthApplication",
      "projectName": "inventory"
    },
    {
      "type": "java",
      "name": "Spring Boot-InventoryApplication<inventory>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.inventory.inventory.InventoryApplication",
      "projectName": "inventory",
      "args": "",
      "envFile": "${workspaceFolder}/.env",
      "vmArgs": " -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.port=53905 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dspring.jmx.enabled=true -Djava.rmi.server.hostname=localhost -Dspring.application.admin.enabled=true -Dspring.boot.project.name=inventory"
    }
  ]
}
